{
  "compilerOptions": {
    "composite": true,
    "outDir": "../../ts-out/workers",
    "target": "ES2021",
    "useDefineForClassFields": true,
    "lib": [
      "Webworker",
      "Webworker.Iterable",
      "WebWorker.ImportScripts",
      "ES2021"
    ],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": ["vite/types/importMeta"]
  },
  "include": ["src"],
  "exclude": ["node_modules", "setuptests.ts", "**/*.spec.ts"]
}
