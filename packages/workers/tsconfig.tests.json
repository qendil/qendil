{
  "compilerOptions": {
    "composite": true,
    "outDir": "../../ts-out/tests/workers",
    "target": "ES2021",
    "useDefineForClassFields": true,
    "lib": [],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": ["vite/types/importMeta", "vitest/globals", "../../node"],
    "noEmit": true
  },
  "include": ["src", "setuptests.ts", "tests.d.ts"],
  "exclude": ["node_modules"]
}
